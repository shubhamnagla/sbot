<launch>

<rosparam file="$(find sbot_description)/launch/controller.yaml" command="load"/>
<node args="right_wheel_joint_position_controller left_wheel_joint_position_controller joint_state_controller " name="controller_spawner" ns="sbot" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/sbot/joint_states"/>
</node>

</launch>